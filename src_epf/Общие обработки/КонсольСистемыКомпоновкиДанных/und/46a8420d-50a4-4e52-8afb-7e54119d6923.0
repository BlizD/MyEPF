		Стр = СтрЗаменить(Стр, Символы.ПС, Символы.ПС + Символы.Таб);
		Стр = Символы.Таб + Стр;
		Результат = Результат + Символы.ПС + Стр;
		
	КонецЕсли;
	
	Результат = Результат + Символы.ПС + ""</resultAndDetailsInfo>"";
	
	Возврат Результат;
	
КонецФункции

