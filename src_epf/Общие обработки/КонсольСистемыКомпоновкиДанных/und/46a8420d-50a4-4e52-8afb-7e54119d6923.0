	ИндексИмени = 1;
	
	Пока Истина Цикл
		
		Имя = БазоваяЧастьИмени + ?(ИндексИмени > 1, "" "" + Строка(ИндексИмени), """");
		Если УникальныеИмена.Получить(Имя) <> Истина Тогда
			
